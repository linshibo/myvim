INCLUDE = -I/usr/include/glib-2.0 -I/usr/lib/glib-2.0/include/ -I./include  -I/usr/include/ -I/usr/local/include/async_serv/
FLAG= -Wall -DNDEBUG -g -fPIC -MMD
SRC = dbproxy.o dispatcher.o online.o switch.o 
libswitch.so:$(SRC) 
	g++ -shared -o libswitch.so  $(SRC) 
%.o:%.cpp 
	g++ $(FLAG) $(INCLUDE) -c  -o $@ $< 
