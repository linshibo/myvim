#ifndef  MOLE2_SWITCH_H
#define  MOLE2_SWITCH_H
#include "mole2.h"

class set_global_data_cnt_in: public Cmessage {
	public:
		/*全局数据类型号 ：0 幸运星兑换数量*/
		uint32_t		type;

		set_global_data_cnt_in();
		void init();
		bool read_from_buf(byte_array_t & ba );
		bool write_to_buf(byte_array_t & ba );
};

	
class set_global_data_cnt_out: public Cmessage {
	public:
		/*相关数据*/
		uint32_t		count;

		set_global_data_cnt_out();
		void init();
		bool read_from_buf(byte_array_t & ba );
		bool write_to_buf(byte_array_t & ba );
};

	
#endif // MOLE2_SWITCH_H
