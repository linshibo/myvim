#!/usr/bin/python
import sys
fp=open(sys.argv[1])
for line in fp.readlines()  :
	map={}
	arr=line.split();
	uid=int(arr[0])
	fpp=open(sys.argv[2])
	for ll in fpp.readlines():
		a=ll.split();
		u=int(a[0])
		i=a[1]
		c=int(a[2])
		if uid == u :
			if not map.has_key(i):
				map[i]=0
			map[i]+=c

	for item in map.iterkeys():
		print arr[0]," ",item," ",map[item]

