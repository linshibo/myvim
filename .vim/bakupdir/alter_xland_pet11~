#!/bin/bash
my='mysql -uroot -proot'
for db in `seq 0 99`
do
        db=`printf %02d $db`
        echo "start db : $db"
        #$my -e  "create database db_user_info_$db"
        for tb in `seq 0 99`
        do
                echo "start tb $tb"
                tb=`printf %02d $tb`
                echo "alter table XLAND_$db.t_pet_$tb 
				modify column experience bigint unsigned NOT NULL default 0; " | $my
        done
done
