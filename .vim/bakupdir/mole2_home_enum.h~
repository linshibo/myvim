#ifndef  MOLE2_HOME_ENUM_H
#define  MOLE2_HOME_ENUM_H

/*命令定义*/	
enum  enum_mole2_home_cmd{
	 home_ex_xxx_cmd		=	1001, /**/
	 home_game_get_map_id_cmd		=	1380, /**/
	 home_game_noti_left_time_cmd		=	1381, /*等待时间*/
	 home_game_del_user_cmd		=	1382, /*用户被打出场了*/
	 home_game_noti_start_cmd		=	1383, /*通知游戏开始*/
	 home_game_noti_info_cmd		=	1384, /**/
	 home_game_noti_pvp_winner_cmd		=	1385, /**/

};

/*错误码定义*/	
enum  enum_mole2_home_error{
	 _HOME_SYS_ERR		=	1001, /*系统出错*/

};



#endif // MOLE2_HOME_ENUM_H
