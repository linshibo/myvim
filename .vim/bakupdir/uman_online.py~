# -*- coding: utf-8 -*-
import ultraman_online_proto
import struct
import mypub
class  Cultraman_online (ultraman_online_proto.Cultraman_online_proto):
	def login(self,userid,key):
		self.set_userid(userid);
		primsg=struct.pack(">16s",key);
		sendbuf=self.pack_with_buf(1001,primsg);
		self.getsock().send(sendbuf)
		pass

