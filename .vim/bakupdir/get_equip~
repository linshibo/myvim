#!/bin/bash
my='mysql -uroot -pta0mee'
result=0
for db in `seq 65 66`
do
        db=`printf %02d $db`
        echo "start process db : $db"
        for tb in `seq 0 99`
        do
                echo "process tb $tb"
                tb=`printf %02d $tb`
                cnt=$( echo "select count(*) from MOLE2_USER_$db.t_attire_$tb where attire_id=147017;" | $my -s )
				echo "cnt=$cnt"
				let "result += cnt"
        done
done
echo "result: $result"
