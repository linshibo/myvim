#!/usr/bin/python 
#FileName:am.py
import sys
fp=open(sys.argv[2])
line=fp.readlines()
print "<Types>"
for l in line  :
	col=l.split();
	if sys.argv[1] == "pet" :
	    print """<Type ID="%u" Name="%s" MiniLV="%u" Race="%u" HPInit="%u" MPInit="%u" AttackInit="%u"  Def="%u" Mdef="%u" SpeedInit="%u" Style="%u" Nature="%u"  MinAttrParam="%u" MaxAttrParam="%u" HitInit="%u" AvoidInit="%u" Critical_attack="%u" Deadly_attack="%u" Rpoison="%u" Rconfusion="%u" Rstone="%u" Rfreeze="%u" Rsleep="%u" Rcurse="%u" Fight_back="%u" Strike_back="%u" Strike_back_hurt="%u" Rcritical_attack="%u" Rdeadly_attack="%u" >
	<TalentSkill ID=""/>
<\Type>
		"""%(int(col[0]), col[1], int(col[2]), int(col[3]), int(col[4]), int(col[5]), int(col[6]),
			int(col[7]), int(col[8]), int(col[9]), int(col[10]), int(col[11]), int(col[12]), int(col[13]),
			int(col[14]), int(col[15]), int(col[16]), int(col[17]), int(col[18]), int(col[19]), int(col[20]),
			int(col[21]), int(col[22]), int(col[23]), int(col[24]), int(col[25]), int(col[26]), int(col[27]), int(col[28]))
print "<\Types>"

