# -*- coding: utf-8 -*-
import xland_online_proto
import struct
import mypub
class  Cxland_online (xland_online_proto.Cxland_online_proto ):
	def login(self,userid,key,onlineid):
		self.set_userid(userid);
		primsg=struct.pack(">L16s",onlineid,key);
		sendbuf=self.pack_with_buf(1001,primsg);
		self.getsock().send(sendbuf)
		pass

