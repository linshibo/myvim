#!/bin/bash
my='mysql -uroot -pta0mee'
for db in `seq 0 99`
do
        db=`printf %02d $db`
        for tb in `seq 0 99`
        do
                echo "process db:$db tb:$tb"
                tb=`printf %02d $tb`
                echo "alter table USER_INFO_$db.t_friend_$tb change create_role_time role_tm int unsigned not null;" | $my
        done
done
