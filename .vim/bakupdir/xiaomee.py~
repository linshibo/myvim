#!/usr/bin/python 
import sys

fp=open(sys.argv[1])
dbname_fix="MOLE2_USER"
tablename_fix="t_user"
count=0
for line in fp.readlines():
	arr=line.split();
	uid=arr[0]
	i=int(uid)%100
	j=int(uid)%10000/1000
	count+=1
	dbname= "%s_%02d"%(dbname_fix,i)
	print """select userid,xiaomee from %s.%s_%d where userid=%s; """ %(dbname,tablename_fix,j,uid)
	if count%20 == 0 :
		print """sleep 2"""
