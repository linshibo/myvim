#include "lua.h"
#include "lualib.h"
#include "lauxlib.h"

lua_State* L;

int luaadd ( int x, int y ) 
{ int sum;
	/* the function name */
	lua_getglobal(L, "add");
	/* the first argument */
	lua_pushnumber(L, x);
	/* the second argument */
	lua_pushnumber(L, y);
	/* call the function with 2 arguments, return 1 result */
	lua_call(L, 2, 1);
	/* get the result */
	sum = (int)lua_tonumber(L, -1);
	lua_pop(L, 1);
	return sum;
} 

int main ( int argc, char *argv[] ) { int sum;
	L = lua_open();
	lua_libopen(L);
	lua_dofile(L, "./conf.lua");
	return 0;
}
