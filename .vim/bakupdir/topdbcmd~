#!/bin/bash
if [ "$1" == "" ] 
then
	echo "please enter log address";
else
	grep "I:....:" "$1" | awk '{print $6}' |sed 's/I:\(....\):.*/\1/'|  awk '{CMD[$1]++;count++;}END { for (a in CMD)print CMD[a] " " CMD[a]/count*100 "% " a;}' | grep -v "./" | column -c3 -s " " -t | sort -nr | nl |  head -n20
fi
