#include <iostream>
#include <vector>
using namespace std;
 
int main()
{
    vector<int> arr;
    arr.push_back(6);
    arr.push_back(7);
    arr.push_back(8);
    arr.push_back(9);


    for(auto i = arr.begin(); i != arr.end(); ++ i) {
        cout << "..." << *i << endl;
    }

    for(vector<int>::iterator it=arr.begin(); it!=arr.end(); ++it)
    {
        
            cout << *it << endl;
        if (*it = 8) {
            cout << "----" << *it << "~~~";
            it = arr.erase(it);
            cout << *it << endl;
        }
    }
    for(auto i = arr.begin(); i != arr.end(); ++ i) {
        cout << "..." << *i << endl;
    }
}
