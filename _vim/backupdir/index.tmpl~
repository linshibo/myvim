    function UserAdd(num, type)
    {
        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange=function()
        {
            if (xmlhttp.readyState==4 && xmlhttp.status==200)
            {
                document.getElementById("Ret").innerHTML=xmlhttp.responseText
            }
            else{
                document.getElementById("Ret").innerHTML="出错啦"
            }
        }
        xmlhttp.open("POST","/api/UserAdd",true);
        xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
        var msg={
            "platform":document.getElementById("platform").value, 
            "name":document.getElementById("userName").value, 
            "type":parseInt(type),
            "value":parseInt(document.getElementById(num).value),
        }
        xmlhttp.send(JSON.stringify(msg))
    }
