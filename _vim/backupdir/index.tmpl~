    function Startgame()
    {
      var xmlhttp = new XMLHttpRequest();
       xmlhttp.onreadystatechange=function()
       {
          if (xmlhttp.readyState==4 && xmlhttp.status==200)
          {
           document.getElementById("Rot").innerHTML=xmlhttp.responseText
       }
                                                                        
       else{
           document.getElementById("Rot").innerHTML="出错啦"
           }
       }  
       xmlhttp.open("POST","/api/Startgame",true);
       xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
       var msg={
          "name":document.getElementById("startgame").value
                                                                                                                                                     
           }
           xmlhttp.send(JSON.stringify(msg))
                                                                                                                                                                             
    }


    function Stopgame()
    {
      var xmlhttp = new XMLHttpRequest();
       xmlhttp.onreadystatechange=function()
       {
          if (xmlhttp.readyState==4 && xmlhttp.status==200)
          {
          document.getElementById("Rot").innerHTML=xmlhttp.responseText
       }
                                                                                                          
       else{
           document.getElementById("Rot").innerHTML="出错啦"
           }
       }  
       xmlhttp.open("POST","/api/Stopgame",true);
       xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
       var msg={
          "name":document.getElementById("stopgame").value
           }
           xmlhttp.send(JSON.stringify(msg))
   }

