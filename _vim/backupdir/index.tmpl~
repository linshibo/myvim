    function SetGuanqia()
    {
        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange=function()
        {
            if (xmlhttp.readyState==4 && xmlhttp.status==200)
            {
                document.getElementById("Ret").innerHTML=xmlhttp.responseText
            }
            else{
                document.getElementById("Ret").innerHTML="出错啦"
            }
        }
        xmlhttp.open("POST","/api/SetGuanqia",true);
        xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
        var msg={"name":document.getElementById("userName").value, 
            "platform":document.getElementById("platform").value,
            "chapter":parseInt(document.getElementById("chapter").value),
            "c_guanqia": parseInt(document.getElementById("c_guanqia").value),
            "s_guanqia": parseInt(document.getElementById("s_guanqia").value),
            "db":0
        }
        xmlhttp.send(JSON.stringify(msg))
    }
    function QueryUserInfo()
    {
        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange=function()
        {
            if (xmlhttp.readyState==4 && xmlhttp.status==200)
            {
                document.getElementById("playInfoRet").innerHTML=xmlhttp.responseText
            }
            else{
                document.getElementById("Ret").innerHTML="出错啦"
            }
        }
        xmlhttp.open("POST","/api/QueryUserInfo",true);
        xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
        var msg={"name":document.getElementById("userName").value, 
            "platform":document.getElementById("platform").value
        }
        xmlhttp.send(JSON.stringify(msg))
    }
    function CheckUserOnline()
    {
        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange=function()
        {
            if (xmlhttp.readyState==4 && xmlhttp.status==200)
            {
                document.getElementById("onlineRet").value=xmlhttp.responseText
            }
        }
        xmlhttp.open("POST","/api/CheckUserOnline",true);
        xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
        var msg={"name":document.getElementById("userName").value, 
            "platform":document.getElementById("platform").value
        }
        xmlhttp.send(JSON.stringify(msg))
    }
    function GetRemain()
    {
        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange=function()
        {
            if (xmlhttp.readyState==4 && xmlhttp.status==200)
            {
                document.getElementById("Ret").innerHTML=xmlhttp.responseText
            }
            else{
                document.getElementById("Ret").innerHTML="出错啦"
            }
        }
        xmlhttp.open("POST","/api/GetRemain",true);
        xmlhttp.setRequestHeader("Content-type","application/x-www-form-urlencoded");
        var msg={
            "platform":document.getElementById("platform").value,
            "year":parseInt(document.getElementById("year").value),
            "mon":parseInt(document.getElementById("mon").value),
            "day":parseInt(document.getElementById("day").value),
            "N":parseInt(document.getElementById("N").value),
            "M":parseInt(document.getElementById("M").value)
        }
        xmlhttp.send(JSON.stringify(msg))
    }
    function GetGuanqiaStat()
